#
# True if this file will be used to shorten stuff
#
active: true

#
# True if we won't mini min files 
#
# Recommended true
#
ignoreMin: true

export: test/javascript.js
compressing: 
- test/test.js
- test/jquery.min.js
- test/test2.js

# 
#  Choose a compilation level
#  - WHITESPACE_ONLY
#  - SIMPLE_OPTIMIZATIONS
#  - ADVANCED_OPTIMIZATIONS
# 
#  More information here: https://developers.google.com/closure/compiler/docs/compilation_levels

compilationLevel: SIMPLE_OPTIMIZATIONS